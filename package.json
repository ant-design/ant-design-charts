{
  "name": "charts",
  "private": true,
  "scripts": {
    "start": "pnpm run build:lib && pnpm -r --stream --filter=./site run start",
    "test": "pnpm -r --stream --filter=./packages/* run test",
    "test:e2e": "pnpm exec playwright test",
    "lint": "pnpm -r --stream --filter=./packages/* run lint",
    "build": "pnpm -r --stream --filter=!./site run build",
    "build:site": "pnpm -r --stream --filter=./site run build",
    "build:lib": "pnpm -r --stream --filter=!./site run build:lib",
    "dev:graphs": "cd packages/graphs && pnpm run dev",
    "profile": "webpack --config webpack.config.js --mode production --profile --json > stats.json",
    "prettier": "prettier --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "ci:version": "pnpm changeset version",
    "add:changelog": "pnpm changeset",
    "before:release": "pnpm run build",
    "release": "pnpm publish --no-git-checks -r --registry=https://registry.npmjs.org --filter @ant-design/*",
    "release:alpha": "pnpm run before:release && pnpm publish --tag alpha --no-git-checks -r --filter @ant-design/*",
    "release:beta": "pnpm run before:release && pnpm publish --tag beta --no-git-checks -r --filter @ant-design/*",
    "publish": "pnpm publish -r --publish-branch v2",
    "changelog": "pnpm -r --stream --filter=!./site run changelog"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/generator": "^7.27.0",
    "@babel/plugin-transform-typescript": "^7.27.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@changesets/cli": "^2.27.9",
    "@playwright/test": "^1.49.1",
    "@swc/jest": "^0.2.37",
    "@types/enzyme": "^3.10.18",
    "@types/node": "^14.18.63",
    "@types/webpack-env": "^1.18.5",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@umijs/fabric": "^2.14.1",
    "babel-loader": "^8.4.1",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^10.0.1",
    "babel-types": "^6.26.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "ejs": "^3.1.10",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.56.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-extended": "^4.0.2",
    "jest-less-loader": "^0.1.2",
    "less": "^4.2.0",
    "less-loader": "^10.2.0",
    "lodash": "^4.17.21",
    "np": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.3",
    "pretty-quick": "^3.3.1",
    "react-dev-utils": "^12.0.1",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "vfile-reporter": "^7.0.5",
    "webpack": "^5.96.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "whatwg-fetch": "^3.6.20",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "pretty-quick --staged"
  },
  "sideEffects": false,
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.27.5",
    "conventional-changelog-cli": "^5.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "less-plugin-npm-import": "^2.1.0",
    "recast": "^0.23.11",
    "remark-frontmatter": "^4.0.1",
    "remark-parse": "^10.0.2",
    "remark-stringify": "^10.0.3",
    "to-vfile": "^7.2.4",
    "tslib": "2.8.1",
    "unified": "^10.1.2"
  },
  "repository": "https://github.com/ant-design/ant-design-charts.git"
}
