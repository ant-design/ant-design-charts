{
  "name": "charts",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "build:site",
    "build": "npm-run-all --parallel build:*",
    "build:plots": "tnpm run build --prefix ./packages/plots",
    "build:graphs": "tnpm run build --prefix ./packages/graphs",
    "build:maps": "tnpm run build --prefix ./packages/maps",
    "build:flowchart": "tnpm run build --prefix ./packages/flowchart",
    "build:charts": "tnpm run build --prefix ./packages/charts",
    "build:site": "tnpm run start --prefix  ./packages/site",
    "profile": "webpack --config webpack.config.js --mode production --profile --json > stats.json",
    "prettier": "prettier --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "bootstrap": "lerna bootstrap"
  },
  "devDependencies": {
    "@antv/data-set": "^0.11.8",
    "@babel/core": "^7.11.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/enzyme": "^3.10.5",
    "@types/node": "^14.0.10",
    "@types/react": "^17.0.24",
    "@types/webpack-env": "^1.14.1",
    "@umijs/fabric": "^2.0.7",
    "babel-loader": "^8.2.2",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^10.0.0",
    "babel-types": "^6.26.0",
    "chalk": "^4.1.0",
    "ejs": "^3.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^7.32.0",
    "fork-ts-checker-webpack-plugin": "^5.2.1",
    "gh-pages": "^3.0.0",
    "jest-canvas-mock": "^2.2.0",
    "lerna": "^4.0.0",
    "lodash": "^4.17.20",
    "np": "*",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "pretty-quick": "^3.0.1",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.3",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "whatwg-fetch": "^3.0.0",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.4",
    "react-dom": ">=16.8.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHooks": {
    "pre-commit": "pretty-quick --staged"
  },
  "np": {
    "yarn": false,
    "contents": "dist"
  },
  "sideEffects": false,
  "license": "MIT"
}
