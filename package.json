{
  "name": "charts",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "site": "cd ./packages/site && npm run start",
    "flow": "cd ./packages/flowchart && npm run build",
    "start": "lerna run  start --scope @ant-design/charts-site --stream",
    "test": "lerna run test --stream",
    "build": "lerna run build --ignore @ant-design/maps --stream --profile --profile-location=logs/profile/",
    "profile": "webpack --config webpack.config.js --mode production --profile --json > stats.json",
    "prettier": "prettier --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "bootstrap": "lerna bootstrap"
  },
  "devDependencies": {
