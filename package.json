{
  "name": "@ant-design/charts",
  "version": "1.0.0",
  "description": "React translate g2plot",
  "files": [
    "/lib",
    "/es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ant-design/ant-design-charts.git"
  },
  "bugs": {
    "url": "https://github.com/ant-design/ant-design-charts/issues"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "dumi dev",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "deploy": "gh-pages -d dist",
    "docs:build": "dumi build",
    "test:coverage": "umi-test --coverage",
    "build": "father-build",
    "ci": "npm run build && npm run test:coverage",
    "pub": "npm run build && np",
    "addChart": "node ./scripts/addChart.js",
    "entry": "node ./scripts/entry.js",
    "mockTest": "node ./scripts/mockTest.js"
  },
  "dependencies": {
    "@antv/g2plot": "^2.0.0",
    "@antv/g6": "^3.6.0",
    "@antv/util": "^2.0.9",
    "react-content-loader": "^5.0.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@types/enzyme": "^3.10.5",
    "@types/node": "^14.0.10",
    "@types/webpack-env": "^1.14.1",
    "@umijs/fabric": "^2.0.7",
    "@umijs/preset-react": "^1.4.8",
    "@umijs/test": "^3.0.16",
    "dumi": "^1.1.0-beta.18",
    "dumi-theme-default": "^1.0.0-beta.5",
    "ejs": "^3.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "escodegen": "^2.0.0",
    "esprima": "^4.0.1",
    "estraverse": "^5.1.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "jest-canvas-mock": "^2.2.0",
    "loadsh": "^0.0.4",
    "np": "*",
    "prettier": "^2.0.2",
    "shelljs": "^0.8.3",
    "typescript": "^3.7.4",
    "whatwg-fetch": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "np": {
    "yarn": false,
    "contents": "dist"
  },
  "publishConfig": {
    "access": "public"
  }
}
