      url: url,
      method: method,
      data: data,
      header: headers,
      complete: function complete(res) {
        signal === null || signal === void 0 ? void 0 : signal.removeEventListener("abort", abortListener);

        if (!res.statusCode) {
          reject(new Error(res.errMsg));
          return;
