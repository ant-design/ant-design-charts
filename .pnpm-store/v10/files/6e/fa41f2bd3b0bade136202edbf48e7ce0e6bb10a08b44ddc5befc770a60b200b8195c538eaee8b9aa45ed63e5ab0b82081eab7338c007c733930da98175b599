  function WS(url, protocol) {
    var _this = _super.call(this) || this;

    _this._readyState = WS.CLOSED;

    if (!url) {
      throw new TypeError("Failed to construct 'WebSocket': url required");
    }

    _this._url = url;
    _this._protocol = protocol;
    return _this;
  }

  (0, _defineProperty.default)(WS.prototype, "url", {
    get: function get() {
      return this._url;
    },
    enumerable: false,
    configurable: true
  });
  (0, _defineProperty.default)(WS.prototype, "protocol", {
    get: function get() {
      return this._protocol;
    },
    enumerable: false,
    configurable: true
  });
  (0, _defineProperty.default)(WS.prototype, "readyState", {
