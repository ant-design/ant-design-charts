"use strict";
Object.defineProperty(exports, "__esModule", { value: true });
exports.getCanvasContext = void 0;
var ctx;
/**
 * 获取 canvas context
 */
var getCanvasContext = function () {
    if (!ctx) {
        ctx = document.createElement('canvas').getContext('2d');
    }
    return ctx;
};
exports.getCanvasContext = getCanvasContext;
