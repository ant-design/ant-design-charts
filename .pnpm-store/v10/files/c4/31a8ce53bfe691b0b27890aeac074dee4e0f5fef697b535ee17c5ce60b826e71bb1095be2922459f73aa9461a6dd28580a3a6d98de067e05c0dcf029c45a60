  ],
  "scripts": {
    "build:cjs": "rm -rf cjs && tsc -p ./tsconfig.json  --module commonjs --outDir cjs",
    "build:esm": "rm -rf esm && tsc -p ./tsconfig.json  --module esnext --outDir esm",
    "build": "npm run build:cjs && npm run build:esm"
  },
