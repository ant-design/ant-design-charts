import type { Interaction as G2Interaction, InteractionTypes as G2InteractionTypes } from '@antv/g2';
import type { DrillDownOptions } from '../interaction/drillDown';
export type Interaction = G2Interaction | DrillDownInteraction;
export type InteractionTypes = G2InteractionTypes | 'drillDown';
export type DrillDownInteraction = DrillDownOptions & {
    type?: 'drillDown';
};
