import React from 'react';
export interface ChartLoadingConfig {
    /**
     * @title 主题
     * @description 配置主题颜色
     */
    theme?: string | object;
    /**
     * @title 加载模板
     * @description 图表加载
     */
    loadingTemplate?: React.ReactElement;
}
export declare const ChartLoading: ({ loadingTemplate, theme }: ChartLoadingConfig) => JSX.Element;
