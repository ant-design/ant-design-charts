import type { DisplayObject } from '../display-objects';
import type { IElement, IEventTarget, INode } from '../dom';
import type { CanvasLike } from '../types';
export declare function isElement(target: IEventTarget | INode | IElement): target is IElement;
export declare function sortedIndex(array: IElement[], value: IElement): number;
export declare function sortByZIndex(o1: IElement, o2: IElement): number;
export declare function findClosestClipPathTarget(object: DisplayObject): DisplayObject;
export declare function setDOMSize($el: CanvasLike, width: number, height: number): void;
export declare function getStyle($el: HTMLElement | CanvasLike, property: string): string;
export declare function getWidth($el: HTMLElement | CanvasLike): number;
export declare function getHeight($el: HTMLElement | CanvasLike): number;
