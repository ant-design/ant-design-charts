import type { TreeSelectProps } from 'antd';
import type { RefSelectProps } from 'antd/lib/select';
import React from 'react';
import type { ProFormFieldItemProps, ProFormFieldRemoteProps } from '../../typing';
export type ProFormTreeSelectProps<T = any> = ProFormFieldItemProps<TreeSelectProps<T> & {
    /**
     * 当搜索关键词发生变化时是否请求远程数据
     *
     * @default true
     */
    fetchDataOnSearch?: boolean;
}, RefSelectProps> & ProFormFieldRemoteProps;
declare const WarpProFormTreeSelect: React.FC<ProFormTreeSelectProps>;
export default WarpProFormTreeSelect;
