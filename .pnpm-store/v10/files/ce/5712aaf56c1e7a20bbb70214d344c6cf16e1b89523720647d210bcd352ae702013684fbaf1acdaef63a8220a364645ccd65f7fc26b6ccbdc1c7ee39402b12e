import { AbstractRendererPlugin } from '@antv/g-lite';
import type { DragndropPluginOptions } from './interfaces';
export declare class Plugin extends AbstractRendererPlugin {
    private options;
    name: string;
    constructor(options?: Partial<DragndropPluginOptions>);
    init(): void;
    destroy(): void;
    setOptions(options: Partial<DragndropPluginOptions>): void;
}
