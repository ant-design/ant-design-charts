   */function yi(t,e,n,r){var i=t-n,a=e-r;return Math.sqrt(i*i+a*a)}function gi(t,e){var n=Math.min.apply(Math,p(t)),r=Math.min.apply(Math,p(e));return{x:n,y:r,width:Math.max.apply(Math,p(t))-n,height:Math.max.apply(Math,p(e))-r}}function mi(t,e,n,r,i,a){return n*Math.cos(i)*Math.cos(a)-r*Math.sin(i)*Math.sin(a)+t}function ki(t,e,n,r,i,a){return n*Math.sin(i)*Math.cos(a)+r*Math.cos(i)*Math.sin(a)+e}function Ei(t,e,n,r,i,a,o){for(var s=function(t,e,n){return Math.atan(-e/t*Math.tan(n))}(n,r,i),u=1/0,l=-1/0,c=[a,o],h=2*-Math.PI;2*Math.PI>=h;h+=Math.PI){var f=s+h;o>a?f>a&&o>f&&c.push(f):f>o&&a>f&&c.push(f)}for(var d=0;c.length>d;d++){var v=mi(t,0,n,r,i,c[d]);u>v&&(u=v),v>l&&(l=v)}for(var p=function(t,e,n){return Math.atan(e/(t*Math.tan(n)))}(n,r,i),y=1/0,g=-1/0,m=[a,o],k=2*-Math.PI;2*Math.PI>=k;k+=Math.PI){var E=p+k;o>a?E>a&&o>E&&m.push(E):E>o&&a>E&&m.push(E)}for(var x=0;m.length>x;x++){var b=ki(0,e,n,r,i,m[x]);y>b&&(y=b),b>g&&(g=b)}return{x:u,y:y,width:l-u,height:g-y}}function xi(t,e,n,r){return yi(t,e,n,r)}function bi(t,e,n,r,i){return{x:(1-i)*t+i*n,y:(1-i)*e+i*r}}function Ti(t,e,n,r,i){var a=1-i;return a*a*a*t+3*e*i*a*a+3*n*i*i*a+r*i*i*i}function Mi(t,e,n,r){var i,a,o,s=-3*t+9*e-9*n+3*r,u=6*t-12*e+6*n,l=3*e-3*t,c=[];if(ae(s,0))ae(u,0)||0>(i=-l/u)||i>1||c.push(i);else{var h=u*u-4*s*l;ae(h,0)?c.push(-u/(2*s)):h>0&&(a=(-u-(o=Math.sqrt(h)))/(2*s),0>(i=(-u+o)/(2*s))||i>1||c.push(i),0>a||a>1||c.push(a))}return c}function wi(t,e,n,r,i,a,o,s){for(var u=[t,o],l=[e,s],c=Mi(t,n,i,o),h=Mi(e,r,a,s),f=0;c.length>f;f++)u.push(Ti(t,n,i,o,c[f]));for(var d=0;h.length>d;d++)l.push(Ti(e,r,a,s,h[d]));return gi(u,l)}function Si(t){return function(t){if(2>t.length)return 0;for(var e=0,n=0;t.length-1>n;n++){var r=t[n],i=t[n+1];e+=yi(r[0],r[1],i[0],i[1])}return e}(t)}function Ni(t,e,n,r){var i=1-r;return i*i*t+2*r*i*e+r*r*n}function Pi(t,e,n){var r=t+n-2*e;if(ae(r,0))return[.5];var i=(t-e)/r;return i>1||0>i?[]:[i]}function Ci(t,e,n,r,i,a){var o=Pi(t,n,i)[0],s=Pi(e,r,a)[0],u=[t,i],l=[e,a];re