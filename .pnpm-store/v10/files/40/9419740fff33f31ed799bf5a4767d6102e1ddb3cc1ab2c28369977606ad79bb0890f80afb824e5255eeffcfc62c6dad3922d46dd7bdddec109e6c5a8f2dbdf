import { Bitmap } from "../models";
import { errorBase64Url } from "../utils/image";
import { isCanvasNode } from "../utils/nodeType";

/**
 * 将Canvas 解析为图片
 * @param canvas
 */
export var parseCanvasToBitmap = function parseCanvasToBitmap(canvas) {
  if (!isCanvasNode(canvas)) return;
  var _canvas$getBoundingCl = canvas.getBoundingClientRect(),
    width = _canvas$getBoundingCl.width,
    height = _canvas$getBoundingCl.height,
    y = _canvas$getBoundingCl.y,
    x = _canvas$getBoundingCl.x;
  var url;
  try {
    url = canvas.toDataURL();
  } catch (e) {
    var errMsg = e.toString();

    // 存在跨域问题
    if (errMsg.includes('Tainted canvases may not be exported.')) {
      // 跨域问题的显示图片
      url = 'data:image/png;base64,';
    } else url = errorBase64Url;
  }
  var bitmap = new Bitmap({
    url: url,
    width: width,
    height: height,
    y: y,
    x: x
  });
  bitmap.mapBasicInfo(canvas);
  return bitmap;
};