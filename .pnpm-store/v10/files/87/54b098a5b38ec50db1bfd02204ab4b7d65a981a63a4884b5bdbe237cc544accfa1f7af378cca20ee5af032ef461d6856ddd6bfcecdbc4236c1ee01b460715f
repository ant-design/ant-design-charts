import { Primitive } from '@antv/vendor/d3-array';
import { TransformComponent as TC } from '../runtime';
export type SortOptions = {
    by?: string;
    reverse?: boolean;
    channel?: string;
    slice?: number | [number, number];
    ordinal?: boolean;
    reducer?: 'max' | 'min' | 'sum' | 'first' | 'last' | 'mean' | 'median' | ((I: number[], V: Primitive[]) => Primitive);
};
/**
 * Sort marks groups by groups.
 */
export declare const Sort: TC<SortOptions>;
