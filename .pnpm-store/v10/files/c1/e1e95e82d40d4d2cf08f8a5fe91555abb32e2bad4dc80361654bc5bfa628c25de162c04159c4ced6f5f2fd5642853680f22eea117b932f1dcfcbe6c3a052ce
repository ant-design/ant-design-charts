function xC(t,e){var n=e.cx,r=void 0===n?0:n,i=e.cy,o=void 0===i?0:i,a=e.r;t.arc(r,o,a,0,2*Math.PI,!1)}function wC(t,e){var n=e.cx,r=void 0===n?0:n,i=e.cy,o=void 0===i?0:i,a=e.rx,s=e.ry;if(t.ellipse)t.ellipse(r,o,a,s,0,0,2*Math.PI,!1);else{var c=a>s?a:s,l=a>s?1:a/s,u=a>s?s/a:1;t.save(),t.scale(l,u),t.arc(r,o,c,0,2*Math.PI)}}function kC(t,e){var n,r,i=e.x1,o=e.y1,a=e.x2,s=e.y2,c=e.markerStart,l=e.markerEnd,u=e.markerStartOffset,f=e.markerEndOffset,h=0,d=0,p=0,g=0,v=0;c&&Vg(c)&&u&&(n=a-i,r=s-o,v=Math.atan2(r,n),h=Math.cos(v)*(u||0),d=Math.sin(v)*(u||0)),l&&Vg(l)&&f&&(n=i-a,r=o-s,v=Math.atan2(r,n),p=Math.cos(v)*(f||0),g=Math.sin(v)*(f||0)),t.moveTo(i+h,o+d),t.lineTo(a+p,s+g)}function MC(t,e){var n,r,i=e.markerStart,o=e.markerEnd,a=e.markerStartOffset,s=e.markerEndOffset,c=e.d,l=c.absolutePath,u=c.segments,f=0,h=0,d=0,p=0,g=0;if(i&&Vg(i)&&a){var v=Pl(i.parentNode.getStartTangent(),2),m=v[0],y=v[1];n=m[0]-y[0],r=m[1]-y[1],g=Math.atan2(r,n),f=Math.cos(g)*(a||0),h=Math.sin(g)*(a||0)}if(o&&Vg(o)&&s){var b=Pl(o.parentNode.getEndTangent(),2),x=b[0],w=b[1];n=x[0]-w[0],r=x[1]-w[1],g=Math.atan2(r,n),d=Math.cos(g)*(s||0),p=Math.sin(g)*(s||0)}for(var k=0;k<l.length;k++){var M=l[k],O=M[0],E=l[k+1],S=0===k&&(0!==f||0!==h),_=(k===l.length-1||E&&("M"===E[0]||"Z"===E[0]))&&0!==d&&0!==p,A=Pl(S?[f,h]:[0,0],2),T=A[0],P=A[1],C=Pl(_?[d,p]:[0,0],2),N=C[0],L=C[1];switch(O){case"M":t.moveTo(M[1]+T,M[2]+P);break;case"L":t.lineTo(M[1]+N,M[2]+L);break;case"Q":t.quadraticCurveTo(M[1],M[2],M[3]+N,M[4]+L);break;case"C":t.bezierCurveTo(M[1],M[2],M[3],M[4],M[5]+N,M[6]+L);break;case"A":var j=u[k].arcParams,R=j.cx,I=j.cy,D=j.rx,B=j.ry,F=j.startAngle,z=j.endAngle,$=j.xRotation,W=j.sweepFlag;if(t.ellipse)t.ellipse(R,I,D,B,$,F,z,!!(1-W));else{var G=D>B?D:B,Y=D>B?1:D/B,H=D>B?B/D:1;t.translate(R,I),t.rotate($),t.scale(Y,H),t.arc(0,0,G,F,z,!!(1-W)),t.scale(1/Y,1/H),t.rotate(-$),t.translate(-R,-I)}_&&t.lineTo(M[6]+d,M[7]+p);break;case"Z":t.closePath()}}}function OC(t,e){var n,r,i=e.markerStart,o=e.markerEnd,a=