function readJson(
  fileSystem: FileSystem | LegacyResolverFileSystem,
  path2: string,
  callback: ReadJsonCallback
): void {
  if ("readJson" in fileSystem &amp;&amp; fileSystem.readJson) {
    return fileSystem.readJson(path2, callback);
  }
