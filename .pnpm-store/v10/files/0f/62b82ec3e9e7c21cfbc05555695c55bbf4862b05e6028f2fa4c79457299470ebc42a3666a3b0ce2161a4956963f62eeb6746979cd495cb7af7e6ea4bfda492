2],o=e[0]*r,s=e[1]*i,l=e[2]*a,c=e[4]*r,u=e[5]*i,d=e[6]*a,f=e[8]*r,h=e[9]*i,p=e[10]*a,g=o+u+p,v=0;return g>0?(v=2*Math.sqrt(g+1),t[3]=.25*v,t[0]=(d-h)/v,t[1]=(f-l)/v,t[2]=(s-c)/v):o>u&&o>p?(v=2*Math.sqrt(1+o-u-p),t[3]=(d-h)/v,t[0]=.25*v,t[1]=(s+c)/v,t[2]=(f+l)/v):u>p?(v=2*Math.sqrt(1+u-o-p),t[3]=(f-l)/v,t[0]=(s+c)/v,t[1]=.25*v,t[2]=(d+h)/v):(v=2*Math.sqrt(1+p-o-u),t[3]=(s-c)/v,t[0]=(f+l)/v,t[1]=(d+h)/v,t[2]=.25*v),t}function Wt(t,e,n,r,i){var a=e[0],o=e[1],s=e[2],l=e[3],c=a+a,u=o+o,d=s+s,f=a*c,h=a*u,p=a*d,g=o*u,v=o*d,m=s*d,y=l*c,b=l*u,x=l*d,w=r[0],k=r[1],M=r[2],O=i[0],E=i[1],S=i[2],A=(1-(g+m))*w,_=(h+x)*w,T=(p-b)*w,P=(h-x)*k,N=(1-(f+m))*k,C=(v+y)*k,L=(p+b)*M,j=(v-y)*M,R=(1-(f+g))*M;return t[0]=A,t[1]=_,t[2]=T,t[3]=0,t[4]=P,t[5]=N,t[6]=C,t[7]=0,t[8]=L,t[9]=j,t[10]=R,t[11]=0,t[12]=n[0]+O-(A*O+P*E+L*S),t[13]=n[1]+E-(_*O+N*E+j*S),t[14]=n[2]+S-(T*O+C*E+R*S),t[15]=1,t}function Gt(t,e){var n=e[0],r=e[1],i=e[2],a=e[3],o=n+n,s=r+r,l=i+i,c=n*o,u=r*o,d=r*s,f=i*o,h=i*s,p=i*l,g=a*o,v=a*s,m=a*l;return t[0]=1-d-p,t[1]=u+m,t[2]=f-v,t[3]=0,t[4]=u-m,t[5]=1-c-p,t[6]=h+g,t[7]=0,t[8]=f+v,t[9]=h-g,t[10]=1-c-d,t[11]=0,t[12]=0,t[13]=0,t[14]=0,t[15]=1,t}function Ht(t,e,n,r,i){var a=1/Math.tan(e/2);if(t[0]=a/n,t[1]=0,t[2]=0,t[3]=0,t[4]=0,t[5]=a,t[6]=0,t[7]=0,t[8]=0,t[9]=0,t[11]=-1,t[12]=0,t[13]=0,t[15]=0,null!=i&&i!==1/0){var o=1/(r-i);t[10]=(i+r)*o,t[14]=2*i*r*o}else t[10]=-1,t[14]=-2*r;return t}var Yt=Ht;function Vt(t,e,n,r,i,a,o){var s=1/(e-n),l=1/(r-i),c=1/(a-o);return t[0]=-2*s,t[1]=0,t[2]=0,t[3]=0,t[4]=0,t[5]=-2*l,t[6]=0,t[7]=0,t[8]=0,t[9]=0,t[10]=2*c,t[11]=0,t[12]=(e+n)*s,t[13]=(i+r)*l,t[14]=(o+a)*c,t[15]=1,t}var qt=Vt;function Ut(t,e,n,r,i,a,o){var s=1/(e-n),l=1/(r-i),c=1/(a-o);return t[0]=-2*s,t[1]=0,t[2]=0,t[3]=0,t[4]=0,t[5]=-2*l,t[6]=0,t[7]=0,t[8]=0,t[9]=0,t[10]=c,t[11]=0,t[12]=(e+n)*s,t[13]=(i+r)*l,t[14]=a*c,t[15]=1,t}function Xt(t,e,n,r){var i,a,o,s,l,c,u,d,f,h,p=e[0],g=e[1],v=e[2],m=r[0],y=r[1],b=r[2],x=n[0],w=n[1],k=n[2];return Math.abs(p-x)<pt&&Math.abs(g-w)<pt&&Math.abs(v-k)<pt?Mt(t):(u=p-x,d=g-w,f=v-k,i=y*(f*=h=1/Math.sqrt(u*u+d*d+f*f))-b*(d*=h),a=b*(u*=h)-m*f,o=m*d-y*u,(h=Math.sqrt(i*i+a*a+o*o))?(i*=h=1/h,a*=h,o*=h):(i=0,a=0,o=0),s=d*o-f*a,l=f*i-u*o,c=u*a-d*i,(h=Math.sqrt(s*s+l*l+c*c))?(s*=h=1/h,l*=h,c*=h):(s=0,l=0,c=0),t[0]=i,t[1]=s,t[2]=u,t[3]=0,t[4]=a,t[5]=l,t[6]=d,t[7]=0,t[8]=o,t[9]=c,t[10]=f,t[11]=0,t[12]=-(i*p+a*g+o*v),t[13]=-(s*p+l*g+c*v),t[14]=-(u*p+d*g+f*v),t[15]=1,t)}function Kt(t,e,n){return t[0]=e[0]-n[0],t[1]=e[1]-n[1],t[2]=e[2]-n[2],t[3]=e[3]-n[3],t[4]=e[4]-n[4],t[5]=e[5]-n[5],t[6]=e[6]-n[6],t[7]=e[7]-n[7],t[8]=e[8]-n[8],t[9]=e[9]-n[9],t[10]=e[10]-n[10],t[11]=e[11]-n[11],t[12]=e[12]-n[12],t[13]=e[13]-n[13],t[14]=e[14]-n[14],t[15]=e[15]-n[15],t}function Zt(t,e){return t[0]===e[0]&&t[1]===e[1]&&t[2]===e[2]&&t[3]===e[3]&&t[4]===e[4]&&t[5]===e[5]&&t[6]===e[6]&&t[7]===e[7]&&t[8]===e[8]&&t[9]===e[9]&&t[10]===e[10]&&t[11]===e[11]&&t[12]===e[12]&&t[13]===e[13]&&t[14]===e[14]&&t[15]===e[15]}var Qt=At,Jt=Kt,te=Object.freeze({__proto__:null,create:yt,clone:bt,copy:xt,fromValues:wt,set:kt,identity:Mt,transpose:Ot,invert:Et,adjoint:function(t,e){var n=e[0],r=e[1],i=e[2],a=e[3],o=e[4],s=e[5],l=e[6],c=e[7],u=e[8],d=e[9],f=e[10],h=e[11],p=e[12],g=e[13],v=e[14],m=e[15],y=n*s-r*o,b=n*l-i*o,x=n*c-a*o,w=r*l-i*s,k=r*c-a*s,M=i*c-a*l,O=u*g-d*p,E=u*v-f*p,S=u*m-h*p,A=d*v-f*g,_=d*m-h*g,T=f*m-h*v;return t[0]=s*T-l*_+c*A,t[1]=i*_-r*T-a*A,t[2]=g*M-v*k+m*w,t[3]=f*k-d*M-h*w,t[4]=l*S-o*T-c*E,t[5]=n*T-i*S+a*E,t[6]=v*x-p*M-m*b,t[7]=u*M-f*x+h*b,t[8]=o*_-s*S+c*O,t[9]=r*S-n*_-a*O,t[10]=p*k-g*x+m*y,t[11]=d*x-u*k-h*y,t[12]=s*E-o*A-l*O,t[13]=n*A-r*E+i*O,t[14]=g*b-p*w-v*y,t[15]=u*w-d*b+f*y,t},determinant:St,multiply:At,translate:_t,scale:Tt,rotate:function(t,e,n,r){var i,a,o,s,l,c,u,d,f,h,p,g,v,m,y,b,x,w,k,M,O,E,S,A,_=r[0],T=r[1],P=r[2],N=Math.sqrt(_*_+T*T+P*P);return N<pt?null:(_*=N=1/N,T*=N,P*=N,i=Math.sin(n),o=1-(a=Math.cos(n)),s=e[0],l=e[1],c=e[2],u=e[3],d=e[4],f=e[5],h=e[6],p=e[7],g=e[8],v=e[9],m=e[10],y=e[11],b=_*_*o+a,x=T*_*o+P*i,w=P*_*o-T*i,k=_*T*o-P*i,