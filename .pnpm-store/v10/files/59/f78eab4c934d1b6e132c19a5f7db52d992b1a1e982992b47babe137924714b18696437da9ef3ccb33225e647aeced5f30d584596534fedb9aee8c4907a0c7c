"use strict";
Object.defineProperty(exports, "__esModule", { value: true });
exports.midPoint = midPoint;
function midPoint(a, b, t) {
    var ax = a[0];
    var ay = a[1];
    var bx = b[0];
    var by = b[1];
    return [ax + (bx - ax) * t, ay + (by - ay) * t];
}
