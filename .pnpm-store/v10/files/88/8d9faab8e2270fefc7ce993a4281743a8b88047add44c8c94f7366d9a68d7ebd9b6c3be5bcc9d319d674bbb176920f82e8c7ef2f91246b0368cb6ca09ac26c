}
function getNavLink(pathname, navs, menuData) {
  if (isExactNavLink(pathname, navs)) {
    return pathname;
  }
  var matchingMenuItem = findMatchingMenuItem(pathname, menuData);
  if (matchingMenuItem) {
    return matchingMenuItem.link;
  }
  return pathname;
}

