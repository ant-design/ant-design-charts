/**
 * 查找与路径匹配的第一个菜单项
 */
function findMatchingMenuItem(pathname, menuData) {
  if (!menuData || !Array.isArray(menuData) || menuData.length === 0) {
    return null;
  }
  var _iterator = _createForOfIteratorHelper(menuData),
    _step;
  try {
    for (_iterator.s(); !(_step = _iterator.n()).done;) {
