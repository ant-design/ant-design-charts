    export const demoIndex = {
  ids: {{{ids}}},
  getter: {{{getter}}}
};`,
    {
      ids: JSON.stringify(demos == null ? void 0 : demos.map((demo) => demo.id)),
      getter: `() => import(/* webpackChunkName: "${(0, import_utils.generateMetaChunkName)(
        this.resourcePath,
