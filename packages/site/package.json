    "typedoc": "^0.17.6",
    "typedoc-plugin-markdown": "^2.2.11",
    "typescript": "^3.6.5"
  },
  "dependencies": {
    "@ant-design/flowchart": "^1.0.2",
    "@ant-design/graphs": "^1.0.1",
    "@ant-design/maps": "^1.0.1",
    "@ant-design/plots": "^1.0.2",
    "@antv/gatsby-theme-antv": "^1.1.8",
