{
  "private": true,
  "name": "@ant-design/charts-site",
  "version": "0.0.1-beta.4",
  "description": "React Visual component library",
  "keywords": [
    "antv",
    "chart",
    "map",
    "graph",
    "graph editor",
    "site"
  ],
  "homepage": "https://charts.ant.design",
  "bugs": {
    "url": "https://github.com/ant-design/ant-design-charts/issues"
  },
  "repository": {
