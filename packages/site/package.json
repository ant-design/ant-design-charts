{
  "private": true,
  "name": "@ant-design/charts-site",
  "version": "0.0.1-beta.4",
  "description": "React Visual component library",
  "keywords": [
    "antv",
    "chart",
    "map",
    "graph",
    "graph editor",
    "site"
  ],
  "homepage": "https://charts.ant.design",
  "bugs": {
    "url": "https://github.com/ant-design/ant-design-charts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ant-design/ant-design-charts"
  },
  "license": "MIT",
  "author": "https://github.com/orgs/antvis/people",
  "scripts": {
    "site:build": "NODE_OPTIONS=--max_old_space_size=12288 yarn run site:clean && gatsby build --prefix-paths",
    "site:clean": "gatsby clean",
    "site:develop": "cross-env GATSBY=true gatsby develop --open",
    "site:deploy": "npm run site:build && gh-pages -d public",
    "start": "NODE_OPTIONS=--max_old_space_size=8192 npm run site:develop --verbose --watch"
  },
  "devDependencies": {
    "@types/react": "^16.14.8",
    "@types/react-dom": "^16.9.13",
    "cross-env": "^7.0.3",
    "gatsby": "^2.24.40",
    "gh-pages": "^2.1.1",
    "typedoc": "^0.17.6",
    "typedoc-plugin-markdown": "^2.2.11",
    "typescript": "^3.6.5"
  },
  "dependencies": {
    "@ant-design/charts": "^1.3.0-beta.4",
    "@antv/gatsby-theme-antv": "^1.1.8",
    "@antv/util": "^2.0.9",
    "antd": "^4.16.13",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-i18next": "^11.1.0"
  }
}
